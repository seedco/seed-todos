{
  "name": "@seed/todos",
  "version": "1.0.2",
  "description": "Todo parser that uses babylon, Babel's parser",
  "main": "dist/todos.js",
  "scripts": {
    "compile": "babel src/ --out-dir dist",
    "test": "npm run compile && dist/todos --debug 1 'src/**/*.js'",
    "test:file": "npm run compile && dist/todos -o todos.md 'src/**/*.js'",
    "prepublish": "npm run compile",
    "2npm": "publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seedco/seed-todos.git"
  },
  "author": "John Bragg @Seed <john@seed.co>",
  "license": "MIT",
  "dependencies": {
    "babylon": "^6.7.0",
    "chalk": "^1.1.3",
    "glob": "^7.0.3",
    "minimist": "^1.2.0",
    "text-table": "^0.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "publish": "^0.6.0"
  },
  "bin": {
    "todos": "./dist/todos"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  }
}
