  "license": "UNLICENSED",
  "dependencies": {
    "chalk": "^1.1.3",
    "esprima-fb": "^15001.1001.0-dev-harmony-fb",
    "minimist": "^1.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-preset-es2015-node5": "^1.1.0"
  },
  "bin": {
    "todos": "./dist/todos"
  },
  "babel": {
    "presets": [
      "es2015-node5"
    ]
