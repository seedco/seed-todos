{
  "name": "@seed/seed-todos",
  "version": "0.0.7",
  "description": "Todo parser",
  "main": "dist/todos.js",
  "scripts": {
    "compile": "babel src/todos.js --out-file dist/todos.js",
    "test": "npm run compile && dist/todos --debug 1 src/*.js",
    "test:file": "npm run compile && dist/todos -o todos.md src/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seedco/seed-todos.git"
  },
  "author": "",
  "license": "UNLICENSED",
  "dependencies": {
    "babylon": "^6.7.0",
    "chalk": "^1.1.3",
    "minimist": "^1.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0"
  },
  "bin": {
    "todos": "./dist/todos"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  }
}
